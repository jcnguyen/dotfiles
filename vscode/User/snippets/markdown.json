	"Daily Log Template": {
		"prefix": ["log"],
		"body": [
			"### $CURRENT_MONTH_NAME $CURRENT_DATE ($CURRENT_DAY_NAME)",
			"",
			"- Today",
			"  - ${0:log}",
			"",
			"- Review",
			"  - How have I moved my projects forward?",
			"    - Examples: Research, designing, or implementation",
			"  - How have I made my life or my team's life easier?",
			"    - Examples: Automate, discussing processes, documenting processes, implementing processes",
			"  - Have I helped someone today?",
			"    - Examples: Debugging, helping with a project",
			""
		],
		"description": "The daily log template"
	}
